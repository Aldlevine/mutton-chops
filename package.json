{
  "name": "mutton-chops",
  "version": "0.0.6",
  "description": "Another template library, under construction",
  "main": "node_modules/src/mutton-chops.js",
  "scripts": {
    "test": "node_modules/.bin/mocha",
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha",
    "cover-view": "sensible-browser coverage/lcov-report/index.html",
    "docs": "node_modules/.bin/jsdoc -c ./jsdoc.conf.json -t ./node_modules/ink-docstrap/template -r node_modules/src -d documentation",
    "docs-publish": "git subtree push --prefix documentation origin gh-pages"
  },
  "bin": {
    "mutton": "./bin/mutton.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aldlevine/mutton-chops"
  },
  "author": "Aaron Levine",
  "license": "MIT",
  "homepage": "https://github.com/aldlevine/mutton-chops#readme",
  "devDependencies": {
    "coveralls": "^2.11.6",
    "ink-docstrap": "^1.1.0",
    "istanbul": "^0.4.2",
    "jsdoc": "^3.4.0",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0"
  },
  "dependencies": {
    "acorn": "^2.7.0",
    "htmlparser": "^1.7.7",
    "htmlparser2": "^3.9.0",
    "lodash": "^4.0.0",
    "uglify-js": "^2.6.1",
    "yargs": "^3.32.0"
  }
